    $(function(){
      $("#posturl").click(function(){
        var naguru = $("#<%= link.eid %>").val();
        console.log(naguru);
        $.ajax({
          type: "POST",
          url: "/naguru",
          dataType: "text", //dataTypeをちゃんと指定する
          data: {
            naguru: naguru,
          },
        //以下、コールバック関数で戻り値を受け取り、id="result"に表示
        success: function(json) {
        dataType: "json",  //dataTypeをちゃんと指定する
        $('#result').text(json);
        },
        error: function() {
        $('#result').text('error');
        }
        });
      });
    });